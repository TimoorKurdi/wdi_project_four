<form action="/users" method="POST">
  <label>
    Name:
    <input name="name">
  </label>

  <label>
    Email:
    <input name="email">
  </label>

  <label>
    Password:
    <input name="password" type="password">
  </label>

  <label>
    Latitude:
    <input name="latitude">
  </label>

  <label>
    Longitude:
    <input name="longitude">
  </label>

  <input name="authenticity_token" value="<%= form_authenticity_token()%>" type="hidden">

  <button>Submit</button>

</form>